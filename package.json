{
  "name": "gitbook-plugin-multilingual-index",
  "version": "0.1.0",
  "description": "customizing multilingual home pages",
  "main": "index.js",
  "engines": {
    "gitbook": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snowdreams1006/gitbook-plugin-multilingual-index.git"
  },
  "keywords": [
    "gitbook-plugin-multilingual-index",
    "multilingual-index",
    "multiple-language",
    "i18n",
    "index"
  ],
  "gitbook": {
    "properties": {
      "enableSuper": {
        "type": "boolean",
        "title": "inherit from super",
        "required": false,
        "default": true
      },
      "index": {
        "type": "string",
        "title": "default index path",
        "required": false
      }
    }
  },
  "author": {
    "name": "snowdreams1006",
    "email": "snowdreams1006@163.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snowdreams1006/gitbook-plugin-multilingual-index/issues"
  },
  "homepage": "https://snowdreams1006.github.io/gitbook-plugin-multilingual-index/"
}
